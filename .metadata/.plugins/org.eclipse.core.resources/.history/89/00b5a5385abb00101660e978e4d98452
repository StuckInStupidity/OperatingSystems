package fr.darkeye.building;

//Hashmap method
import java.util.*;
class Main {
public static void main(String[] args) {
  HashMap<String,Integer> cp = new HashMap<String,Integer>();
  for(int i = 0; i < 3; i++){
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int c = sc.nextInt();
    int d = sc.nextInt();
    int e = sc.nextInt();
    sc.close();
    int v = e*d*c-(((e-a-a)*(e-a-a)*3*c)/2);
    int cc = v*50;
    if(i==0){
      cp.put("Paris", cc);
    }
    else if(i==1){
      cp.put("Rome", cc);
    }
    else{
      cp.put("London", cc);
    }
  }
System.out.println(cp);
}
}
